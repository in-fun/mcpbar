{
  "name": "exa-mcp-server",
  "version": "0.1.0",
  "description": "A Model Context Protocol server with Exa which does web search",
  "homepage": "https://github.com/MrunmayS/exa-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrunmayS/exa-mcp-server"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "exa-mcp-server",
    "mrunmays"
  ],
  "inputs": [
    {
      "id": "exa_api_key",
      "type": "promptString",
      "description": "Environment variable for exa api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "/path/to/exa-mcp-server/build/index.js"
    ],
    "env": {
      "EXA_API_KEY": "your-api-key-here"
    }
  }
}
