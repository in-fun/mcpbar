{
  "name": "dubco-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for controlling dub.co accounts",
  "homepage": "https://github.com/Gitmaxd/dubco-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gitmaxd/dubco-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "dubco-mcp-server",
    "gitmaxd"
  ],
  "inputs": [
    {
      "id": "dubco_api_key",
      "type": "promptString",
      "description": "Environment variable for dubco api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "dubco-mcp-server",
    "env": {
      "DUBCO_API_KEY": "your_api_key_here"
    },
    "disabled": false,
    "autoApprove": []
  }
}
