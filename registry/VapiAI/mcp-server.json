{
  "name": "@vapi-ai/mcp-server",
  "version": "0.0.6",
  "description": "Vapi MCP Server",
  "homepage": "https://vapi.ai/",
  "repository": {
    "type": "git",
    "url": "https://github.com/VapiAI/mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "ai",
    "mcp",
    "mcp-server",
    "vapi",
    "voice-agents",
    "model-context-protocol"
  ],
  "inputs": [
    {
      "id": "vapi_token",
      "type": "promptString",
      "description": "Environment variable for vapi token authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@vapi-ai/mcp-server"
    ],
    "env": {
      "VAPI_TOKEN": "<your_vapi_token>"
    }
  }
}
