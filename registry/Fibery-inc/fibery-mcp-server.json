{
  "name": "fibery-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for fibery-mcp-server",
  "homepage": "https://github.com/Fibery-inc/fibery-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fibery-inc/fibery-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "fibery-mcp-server",
    "fibery-inc"
  ],
  "inputs": [],
  "server": {
    "command": "uv",
    "args": [
      "tool",
      "run",
      "fibery-mcp-server",
      "--fibery-host",
      "your-domain.fibery.io",
      "--fibery-api-token",
      "your-api-token"
    ]
  }
}
