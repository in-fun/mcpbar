{
  "name": "@gongrzhe/server-travelplanner-mcp",
  "version": "0.1.2",
  "description": "MCP server for travel planning and itinerary management",
  "homepage": "https://github.com/GongRzhe/TRAVEL-PLANNER-MCP-Server",
  "repository": {
    "type": "git",
    "url": "https://github.com/GongRzhe/TRAVEL-PLANNER-MCP-Server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "travel-planner-mcp-server",
    "gongrzhe"
  ],
  "inputs": [
    {
      "id": "google_maps_api_key",
      "type": "promptString",
      "description": "Environment variable for google maps api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "@gongrzhe/server-travelplanner-mcp"
    ],
    "env": {
      "GOOGLE_MAPS_API_KEY": "your_google_maps_api_key"
    }
  }
}
