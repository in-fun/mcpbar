{
  "name": "mcp-jinaai-search",
  "version": "0.0.2",
  "description": "MCP server for JinaAI search",
  "homepage": "https://github.com/spences10/mcp-jinaai-search",
  "repository": {
    "type": "git",
    "url": "https://github.com/spences10/mcp-jinaai-search"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-jinaai-search",
    "spences10"
  ],
  "inputs": [
    {
      "id": "jinaai_api_key",
      "type": "promptString",
      "description": "Environment variable for jinaai api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "-y",
      "mcp-jinaai-search"
    ],
    "env": {
      "JINAAI_API_KEY": "your-jinaai-api-key"
    }
  }
}
