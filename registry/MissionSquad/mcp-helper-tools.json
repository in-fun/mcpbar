{
  "name": "@missionsquad/mcp-helper-tools",
  "version": "1.1.1",
  "description": "MCP server providing encoding, geolocation, generator, datetime, and security tools",
  "homepage": "https://github.com/MissionSquad/mcp-helper-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/MissionSquad/mcp-helper-tools"
  },
  "license": "Apache-2.0",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-helper-tools",
    "missionsquad"
  ],
  "inputs": [
    {
      "id": "node_env",
      "type": "promptString",
      "description": "Environment variable for node env configuration",
      "password": false
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "node_modules/@cyanheads/toolkit-mcp-server/build/index.js"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
}
