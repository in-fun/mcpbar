{
  "name": "web-browser-mcp-server",
  "version": "1.0.0",
  "description": "A Minimum Control Program (MCP) server implementation for web browsing capabilities using BeautifulSoup4",
  "homepage": "https://github.com/blazickjp/web-browser-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/blazickjp/web-browser-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "web-browser-mcp-server",
    "blazickjp"
  ],
  "inputs": [
    {
      "id": "request_timeout",
      "type": "promptString",
      "description": "Environment variable for request timeout configuration",
      "password": false
    }
  ],
  "server": {
    "command": "uv",
    "args": [
      "tool",
      "run",
      "web-browser-mcp-server"
    ],
    "env": {
      "REQUEST_TIMEOUT": "30"
    }
  }
}
