{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Omi conversations",
  "homepage": "https://github.com/Ritesh2351235/Omi-MCP",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ritesh2351235/Omi-MCP"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "omi-mcp",
    "ritesh2351235"
  ],
  "inputs": [
    {
      "id": "node_env",
      "type": "promptString",
      "description": "Environment variable for node env configuration",
      "password": false
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "/path/to/your/mcp-server/dist/server.js"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  }
}
