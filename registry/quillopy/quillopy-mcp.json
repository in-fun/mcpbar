{
  "name": "@quillopy/mcp",
  "version": "0.1.4",
  "description": "MCP server for quillopy - a documentation search tool for programming languages, libraries, and frameworks",
  "homepage": "https://github.com/quillopy/quillopy-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/quillopy/quillopy-mcp"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "quillopy-mcp",
    "quillopy"
  ],
  "inputs": [
    {
      "id": "quillopy_api_key",
      "type": "promptString",
      "description": "Environment variable for quillopy api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@quillopy/mcp"
    ],
    "env": {
      "QUILLOPY_API_KEY": "<your-api-key>"
    }
  }
}
