{
  "name": "mcp-fathom-analytics",
  "version": "1.0.2",
  "description": "MCP server for Fathom Analytics",
  "homepage": "https://www.npmjs.com/package/mcp-fathom-analytics",
  "repository": {
    "type": "git",
    "url": "https://github.com/mackenly/mcp-fathom-analytics"
  },
  "license": "MIT",
  "keywords": [
    "fathom-analytics",
    "mcp",
    "mcp-server",
    "model-context-protocol"
  ],
  "inputs": [
    {
      "id": "fathom_api_key",
      "type": "promptString",
      "description": "Environment variable for fathom api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "mcp-fathom-analytics"
    ],
    "env": {
      "FATHOM_API_KEY": "your_api_key_here"
    }
  }
}
