{
  "name": "markdown-sidecar-mcp",
  "version": "1.0.0",
  "description": "MCP server for markdown-sidecar-mcp",
  "homepage": "https://github.com/speakeasy-api/markdown-sidecar-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/speakeasy-api/markdown-sidecar-mcp"
  },
  "license": "MIT",
  "keywords": [
    "golang",
    "mcp",
    "npm",
    "pypi",
    "model-context-protocol"
  ],
  "inputs": [],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "--package",
      "markdown-sidecar-mcp",
      "--",
      "mcp",
      "start",
      "--workingDir",
      "{REPO_WORKING_DIR}",
      "--packageName",
      "{PACKAGE_NAME}",
      "--registry",
      "npm"
    ]
  }
}
