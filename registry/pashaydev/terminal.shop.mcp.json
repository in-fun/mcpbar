{
  "name": "terminal-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Terminal.shop API",
  "homepage": "https://github.com/pashaydev/terminal.shop.mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/pashaydev/terminal.shop.mcp"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "terminal.shop.mcp",
    "pashaydev"
  ],
  "inputs": [
    {
      "id": "terminal_api_token",
      "type": "promptString",
      "description": "Environment variable for terminal api token authentication",
      "password": true
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "path_to_folder/server.js"
    ],
    "env": {
      "TERMINAL_API_TOKEN": "token here"
    }
  }
}
