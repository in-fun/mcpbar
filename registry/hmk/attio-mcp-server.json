{
  "name": "attio-mcp-server",
  "version": "0.0.2",
  "description": "A Model Context Protocol server that connects Attio to LLMs",
  "homepage": "https://github.com/hmk/attio-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmk/attio-mcp-server"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "attio-mcp-server",
    "hmk"
  ],
  "inputs": [
    {
      "id": "attio_api_key",
      "type": "promptString",
      "description": "Environment variable for attio api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "attio-mcp-server"
    ],
    "env": {
      "ATTIO_API_KEY": "YOUR_ATTIO_API_KEY"
    }
  }
}
