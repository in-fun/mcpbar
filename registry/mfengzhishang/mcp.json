{
  "name": "hyperbrowser-mcp",
  "version": "1.0.25",
  "description": "Hyperbrowser Model Context Protocol Server",
  "homepage": "https://github.com/mfengzhishang/mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfengzhishang/mcp"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp",
    "mfengzhishang"
  ],
  "inputs": [
    {
      "id": "hyperbrowser_api_key",
      "type": "promptString",
      "description": "Environment variable for hyperbrowser api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "hyperbrowser-mcp"
    ],
    "env": {
      "HYPERBROWSER_API_KEY": "YOUR-API-KEY"
    }
  }
}
