{
  "name": "@yosider/cosense-mcp-server",
  "version": "0.2.1",
  "description": "A MCP server for cosense",
  "homepage": "https://www.npmjs.com/package/@yosider/cosense-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/yosider/cosense-mcp-server"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cosense-mcp-server",
    "yosider"
  ],
  "inputs": [
    {
      "id": "cosense_project_name",
      "type": "promptString",
      "description": "Environment variable for cosense project name configuration",
      "password": false
    },
    {
      "id": "cosense_sid",
      "type": "promptString",
      "description": "Environment variable for cosense sid configuration",
      "password": false
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@yosider/cosense-mcp-server"
    ],
    "env": {
      "COSENSE_PROJECT_NAME": "your_project_name",
      "COSENSE_SID": "your_sid"
    }
  }
}
