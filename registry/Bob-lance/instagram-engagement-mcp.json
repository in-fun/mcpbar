{
  "name": "instagram-engagement-mcp",
  "version": "0.1.0",
  "description": "Instagram Engagement Analysis MCP Server for analyzing engagement metrics and identifying leads",
  "homepage": "https://github.com/Bob-lance/instagram-engagement-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bob-lance/instagram-engagement-mcp"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "instagram-engagement-mcp",
    "bob-lance"
  ],
  "inputs": [
    {
      "id": "instagram_username",
      "type": "promptString",
      "description": "Environment variable for instagram username configuration",
      "password": false
    },
    {
      "id": "instagram_password",
      "type": "promptString",
      "description": "Environment variable for instagram password authentication",
      "password": false
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "instagram-engagement-mcp"
    ],
    "env": {
      "INSTAGRAM_USERNAME": "your_instagram_username",
      "INSTAGRAM_PASSWORD": "your_instagram_password"
    },
    "disabled": false,
    "autoApprove": []
  }
}
