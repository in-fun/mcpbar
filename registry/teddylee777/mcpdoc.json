{
  "name": "mcpdoc",
  "version": "1.0.0",
  "description": "Expose llms-txt to IDEs for development",
  "homepage": "https://github.com/teddylee777/mcpdoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/teddylee777/mcpdoc"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcpdoc",
    "teddylee777"
  ],
  "inputs": [],
  "server": {
    "command": "uvx",
    "args": [
      "--from",
      "mcpdoc",
      "mcpdoc",
      "--urls",
      "LangGraph:https://langchain-ai.github.io/langgraph/llms.txt",
      "--transport",
      "stdio",
      "--port",
      "8081",
      "--host",
      "localhost"
    ]
  }
}
