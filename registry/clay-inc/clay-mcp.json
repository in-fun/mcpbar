{
  "name": "@clayhq/clay-mcp",
  "version": "1.0.1",
  "description": "A Model Context Protocol (MCP) server for Clay",
  "homepage": "https://github.com/clay-inc/clay-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/clay-inc/clay-mcp"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "clay-mcp",
    "clay-inc"
  ],
  "inputs": [
    {
      "id": "clay_api_key",
      "type": "promptString",
      "description": "Environment variable for clay api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "@clayhq/clay-mcp@latest"
    ],
    "env": {
      "CLAY_API_KEY": "YOUR_CLAY_API_KEY"
    }
  }
}
