{
  "name": "@modelcontextprotocol/server-postgres",
  "version": "0.6.2",
  "description": "MCP server for interacting with PostgreSQL databases",
  "homepage": "https://github.com/DanielRSnell/postgres-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielRSnell/postgres-mcp"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "postgres-mcp",
    "danielrsnell"
  ],
  "inputs": [],
  "server": {
    "command": "docker",
    "args": [
      "run",
      "-i",
      "--rm",
      "mcp/postgres",
      "postgresql://host.docker.internal:5432/mydb"
    ]
  }
}
