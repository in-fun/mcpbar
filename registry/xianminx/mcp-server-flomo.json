{
  "name": "mcp-server-flomo",
  "version": "0.0.11",
  "description": "A MCP server for Flomo",
  "homepage": "https://github.com/xianminx/mcp-server-flomo",
  "repository": {
    "type": "git",
    "url": "https://github.com/xianminx/mcp-server-flomo"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server-flomo",
    "xianminx"
  ],
  "inputs": [
    {
      "id": "flomo_api_url",
      "type": "promptString",
      "description": "Environment variable for flomo api url configuration",
      "password": false
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "mcp-server-flomo"
    ],
    "env": {
      "FLOMO_API_URL": "your_api_url_here"
    }
  }
}
