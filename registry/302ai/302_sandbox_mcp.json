{
  "name": "@302ai/sandbox-mcp",
  "version": "0.2.0",
  "description": "A Model Context Protocol server",
  "homepage": "https://github.com/302ai/302_sandbox_mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/302ai/302_sandbox_mcp"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "302_sandbox_mcp",
    "302ai"
  ],
  "inputs": [
    {
      "id": "302ai_api_key",
      "type": "promptString",
      "description": "Environment variable for 302ai api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@302ai/sandbox-mcp"
    ],
    "env": {
      "302AI_API_KEY": "YOUR_API_KEY_HERE"
    }
  }
}
