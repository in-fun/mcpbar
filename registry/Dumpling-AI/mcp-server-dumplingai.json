{
  "name": "mcp-server-dumplingai",
  "version": "1.1.3",
  "description": "MCP Server for Dumpling AI providing various Data scraping, conversion, and extraction tools",
  "homepage": "https://github.com/Dumpling-AI/mcp-server-dumplingai",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dumpling-AI/mcp-server-dumplingai"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server-dumplingai",
    "dumpling-ai"
  ],
  "inputs": [
    {
      "id": "dumpling_api_key",
      "type": "promptString",
      "description": "Environment variable for dumpling api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "mcp-server-dumplingai"
    ],
    "env": {
      "DUMPLING_API_KEY": "<your-api-key>"
    }
  }
}
