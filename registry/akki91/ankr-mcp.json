{
  "name": "@akki91/ankr-mcp",
  "version": "0.0.1",
  "description": "MCP server for using Ankr API(s)",
  "homepage": "https://github.com/akki91/ankr-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/akki91/ankr-mcp"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ankr-mcp",
    "akki91"
  ],
  "inputs": [
    {
      "id": "ankr_api_token",
      "type": "promptString",
      "description": "Environment variable for ankr api token authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "@akki91/ankr-mcp"
    ],
    "env": {
      "ANKR_API_TOKEN": "your_api_token_here"
    }
  }
}
