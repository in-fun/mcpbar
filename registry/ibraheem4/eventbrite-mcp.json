{
  "name": "@ibraheem4/eventbrite-mcp",
  "version": "1.0.1",
  "description": "An Eventbrite MCP server for interacting with Eventbrite's API",
  "homepage": "https://github.com/ibraheem4/eventbrite-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/ibraheem4/eventbrite-mcp"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "eventbrite-mcp",
    "ibraheem4"
  ],
  "inputs": [
    {
      "id": "eventbrite_api_key",
      "type": "promptString",
      "description": "Environment variable for eventbrite api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@ibraheem4/eventbrite-mcp"
    ],
    "env": {
      "EVENTBRITE_API_KEY": "your-eventbrite-api-key"
    },
    "disabled": false,
    "autoApprove": []
  }
}
