{
  "name": "uns_mcp",
  "version": "1.0.0",
  "description": "MCP server for UNS-MCP",
  "homepage": "https://github.com/Unstructured-IO/UNS-MCP",
  "repository": {
    "type": "git",
    "url": "https://github.com/Unstructured-IO/UNS-MCP"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "uns-mcp",
    "unstructured-io"
  ],
  "inputs": [
    {
      "id": "unstructured_api_key",
      "type": "promptString",
      "description": "Environment variable for unstructured api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "uvx",
    "args": [
      "uns_mcp"
    ],
    "env": {
      "UNSTRUCTURED_API_KEY": "<your-key>"
    }
  }
}
