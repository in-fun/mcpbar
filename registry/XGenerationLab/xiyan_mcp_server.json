{
  "name": "xiyan_mcp_server",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server that enables natural language queries to databases",
  "homepage": "https://github.com/XGenerationLab/xiyan_mcp_server",
  "repository": {
    "type": "git",
    "url": "https://github.com/XGenerationLab/xiyan_mcp_server"
  },
  "license": "Apache-2.0",
  "keywords": [
    "database",
    "mcp",
    "text-to-sql",
    "model-context-protocol"
  ],
  "inputs": [
    {
      "id": "yml",
      "type": "promptString",
      "description": "Environment variable for yml configuration",
      "password": false
    }
  ],
  "server": {
    "command": "/xxx/python",
    "args": [
      "-m",
      "xiyan_mcp_server"
    ],
    "env": {
      "YML": "PATH/TO/YML"
    }
  }
}
