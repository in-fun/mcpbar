{
  "name": "gitee-mcp-server",
  "version": "0.1.1",
  "description": "MCP Server for using the Gitee API",
  "homepage": "https://www.npmjs.com/package/gitee-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/normal-coder/gitee-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "cline",
    "cursor",
    "gitee",
    "mcp",
    "mcp-server",
    "windsurf",
    "zed",
    "model-context-protocol"
  ],
  "inputs": [
    {
      "id": "gitee_personal_access_token",
      "type": "promptString",
      "description": "Environment variable for gitee personal access token authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "gitee-mcp-server"
    ],
    "env": {
      "GITEE_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN>"
    }
  }
}
