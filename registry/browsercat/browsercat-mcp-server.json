{
  "name": "@browsercatco/mcp-server",
  "version": "0.1.0",
  "description": "MCP server for remote browser automation using BrowserCat",
  "homepage": "https://github.com/browsercat/browsercat-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/browsercat/browsercat-mcp-server"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "browsercat-mcp-server",
    "browsercat"
  ],
  "inputs": [
    {
      "id": "browsercat_api_key",
      "type": "promptString",
      "description": "Environment variable for browsercat api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@browsercatco/mcp-server"
    ],
    "env": {
      "BROWSERCAT_API_KEY": "your-api-key-here"
    }
  }
}
