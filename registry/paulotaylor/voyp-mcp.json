{
  "name": "voyp-mcp",
  "version": "0.1.0",
  "description": "Voyp Model Context Protocol server",
  "homepage": "https://github.com/paulotaylor/voyp-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulotaylor/voyp-mcp"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "voyp-mcp",
    "paulotaylor"
  ],
  "inputs": [
    {
      "id": "voyp_api_key",
      "type": "promptString",
      "description": "Environment variable for voyp api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "voyp-mcp"
    ],
    "env": {
      "VOYP_API_KEY": "your-VOYP-api-key"
    }
  }
}
