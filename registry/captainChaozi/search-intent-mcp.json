{
  "name": "@search-intent/mcp",
  "version": "1.1.0",
  "description": "MCP Demo Server",
  "homepage": "https://github.com/captainChaozi/search-intent-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/captainChaozi/search-intent-mcp"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "search-intent-mcp",
    "captainchaozi"
  ],
  "inputs": [
    {
      "id": "search_intent_api_key",
      "type": "promptString",
      "description": "Environment variable for search intent api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "@search-intent/mcp"
    ],
    "env": {
      "SEARCH_INTENT_API_KEY": "xxx"
    }
  }
}
