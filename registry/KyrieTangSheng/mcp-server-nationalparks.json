{
  "name": "mcp-server-nationalparks",
  "version": "1.0.0",
  "description": "MCP server for mcp-server-nationalparks",
  "homepage": "https://github.com/KyrieTangSheng/mcp-server-nationalparks",
  "repository": {
    "type": "git",
    "url": "https://github.com/KyrieTangSheng/mcp-server-nationalparks"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server-nationalparks",
    "kyrietangsheng"
  ],
  "inputs": [
    {
      "id": "nps_api_key",
      "type": "promptString",
      "description": "Environment variable for nps api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "mcp-server-nationalparks"
    ],
    "env": {
      "NPS_API_KEY": "YOUR_NPS_API_KEY"
    }
  }
}
