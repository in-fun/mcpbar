{
  "name": "lunchmoney-mcp-server",
  "version": "1.1.0",
  "description": "Lunchmoney MCP server for Claude",
  "homepage": "https://github.com/leafeye/lunchmoney-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/leafeye/lunchmoney-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "lunchmoney-mcp-server",
    "leafeye"
  ],
  "inputs": [
    {
      "id": "lunchmoney_token",
      "type": "promptString",
      "description": "Environment variable for lunchmoney token authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "lunchmoney-mcp-server"
    ],
    "env": {
      "LUNCHMONEY_TOKEN": "your_token_here"
    }
  }
}
