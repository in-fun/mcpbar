{
  "name": "mcp-server-vikingdb",
  "version": "1.0.0",
  "description": "a mcp server for vikingdb store and search",
  "homepage": "https://github.com/KashiwaByte/vikingdb-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/KashiwaByte/vikingdb-mcp-server"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "vikingdb-mcp-server",
    "kashiwabyte"
  ],
  "inputs": [],
  "server": {
    "command": "uv",
    "args": [
      "--directory",
      "dir to mcp-server-vikingdb",
      "run",
      "mcp-server-vikingdb",
      "--vikingdb-host",
      "your host",
      "--vikingdb-region",
      "your region",
      "--vikingdb-ak",
      "your access key",
      "--vikingdb-sk",
      "your secret key",
      "--collection-name",
      "your collection name",
      "--index-name",
      "your index name"
    ]
  }
}
