{
  "name": "mcp-server-duckdb",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server implementation for DuckDB, providing database interaction capabilities",
  "homepage": "https://github.com/ktanaka101/mcp-server-duckdb",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktanaka101/mcp-server-duckdb"
  },
  "license": "MIT",
  "keywords": [
    "duckdb",
    "mcp",
    "mcp-server",
    "model-context-protocol"
  ],
  "inputs": [],
  "server": {
    "command": "uvx",
    "args": [
      "mcp-server-duckdb",
      "--db-path",
      "~/mcp-server-duckdb/data/data.db"
    ]
  }
}
