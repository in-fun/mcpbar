{
  "name": "webflow-mcp-server",
  "version": "1.0.0",
  "description": "An MCP server for using Webflow APIs",
  "homepage": "https://github.com/kapilduraphe/webflow-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/kapilduraphe/webflow-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "webflow-mcp-server",
    "kapilduraphe"
  ],
  "inputs": [
    {
      "id": "webflow_api_token",
      "type": "promptString",
      "description": "Environment variable for webflow api token authentication",
      "password": true
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "/ABSOLUTE/PATH/TO/YOUR/build/index.js"
    ],
    "env": {
      "WEBFLOW_API_TOKEN": "your-api-token"
    }
  }
}
