{
  "name": "mcp-server-weread",
  "version": "0.1.0",
  "description": "A WeRead MCP Server for seamless integration with Claude Desktop",
  "homepage": "https://github.com/freestylefly/mcp-server-weread",
  "repository": {
    "type": "git",
    "url": "https://github.com/freestylefly/mcp-server-weread"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server-weread",
    "freestylefly"
  ],
  "inputs": [
    {
      "id": "weread_cookie",
      "type": "promptString",
      "description": "Environment variable for weread cookie configuration",
      "password": false
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "/path/to/mcp-server-weread/build/index.js"
    ],
    "env": {
      "WEREAD_COOKIE": "你的微信读书cookie"
    }
  }
}
