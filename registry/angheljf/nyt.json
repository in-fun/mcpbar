{
  "name": "nyt",
  "version": "0.1.0",
  "description": "A MCP Server",
  "homepage": "https://github.com/angheljf/nyt",
  "repository": {
    "type": "git",
    "url": "https://github.com/angheljf/nyt"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "nyt",
    "angheljf"
  ],
  "inputs": [
    {
      "id": "nytimes_api_key",
      "type": "promptString",
      "description": "Environment variable for nytimes api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "node",
    "args": [
      "path/to/your/build/index.js"
    ],
    "env": {
      "NYTIMES_API_KEY": "your_api_key_here"
    }
  }
}
