{
  "name": "vite_react_shadcn_ts",
  "version": "0.0.0",
  "description": "MCP server for salon-connect-africa",
  "homepage": "https://github.com/Lareference1/salon-connect-africa",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lareference1/salon-connect-africa"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "salon-connect-africa",
    "lareference1"
  ],
  "inputs": [
    {
      "id": "your_git_url",
      "type": "promptString",
      "description": "Environment variable for your git url",
      "password": false
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "vite_react_shadcn_ts"
    ],
    "env": {
      "YOUR_GIT_URL": "${input:your_git_url}"
    }
  }
}
