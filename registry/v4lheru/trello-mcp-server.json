{
  "name": "trello-mcp-server",
  "version": "0.1.0",
  "description": "Trello MCP Server - A Model Context Protocol server for interacting with the Trello API",
  "homepage": "https://github.com/v4lheru/trello-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/v4lheru/trello-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "trello-mcp-server",
    "v4lheru"
  ],
  "inputs": [
    {
      "id": "trello_api_key",
      "type": "promptString",
      "description": "Environment variable for trello api key",
      "password": true
    },
    {
      "id": "trello_token",
      "type": "promptString",
      "description": "Environment variable for trello token",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "trello-mcp-server"
    ],
    "env": {
      "TRELLO_API_KEY": "${input:trello_api_key}",
      "TRELLO_TOKEN": "${input:trello_token}"
    }
  }
}
