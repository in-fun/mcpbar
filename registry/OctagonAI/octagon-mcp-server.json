{
  "name": "octagon-mcp",
  "version": "1.0.15",
  "description": "MCP server for Octagon API. Provides specialized AI agents for investment research of public and private markets.",
  "homepage": "https://docs.octagonagents.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/OctagonAI/octagon-mcp-server"
  },
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "octagon-mcp-server",
    "octagonai"
  ],
  "inputs": [
    {
      "id": "octagon_api_key",
      "type": "promptString",
      "description": "Environment variable for octagon api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "octagon-mcp@latest"
    ],
    "env": {
      "OCTAGON_API_KEY": "YOUR_API_KEY_HERE"
    }
  }
}
