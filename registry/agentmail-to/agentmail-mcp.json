{
  "name": "agentmail-mcp",
  "version": "0.1.3",
  "description": "AgentMail MCP Server",
  "homepage": "https://github.com/agentmail-to/agentmail-mcp",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentmail-to/agentmail-mcp"
  },
  "license": "Unknown",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "agentmail-mcp",
    "agentmail-to"
  ],
  "inputs": [
    {
      "id": "agentmail_api_key",
      "type": "promptString",
      "description": "Environment variable for agentmail api key authentication",
      "password": true
    }
  ],
  "server": {
    "command": "npx",
    "args": [
      "-y",
      "agentmail-mcp"
    ],
    "env": {
      "AGENTMAIL_API_KEY": "YOUR_API_KEY"
    }
  }
}
